package br.com.fiap.Pro;

public class Produto {

	private double peso;

	privatedoublealtura;
	public double getPeso() { 
		return peso; 
		}

	public void setPeso(double peso) { this.peso= peso; }

	public double getAltura() { 
		return altura; }

	public void setAltura(double altura) {
		this.altura = altura;
	}
}

}
